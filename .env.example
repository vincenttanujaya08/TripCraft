# Required API Keys
GEMINI_API_KEY=your_gemini_2.5_flash_key_here

# Optional API Keys (app will fallback to seed data if empty)
OPENTRIPMAP_API_KEY=
UNSPLASH_ACCESS_KEY=
PEXELS_API_KEY=

# Database
DATABASE_URL=sqlite:///./tripcraft.db

# LLM Config
ENABLE_LLM_FALLBACK=true
LLM_MODEL=gemini-2.0-flash-exp
LLM_SEARCH_GROUNDING=true

# Image Config
IMAGE_CACHE_ENABLED=true
IMAGE_MIN_CONFIDENCE_BADGE=0.4
IMAGE_HIGH_CONFIDENCE_THRESHOLD=0.7

# App Config
DEBUG=true
BACKEND_PORT=8000
FRONTEND_URL=http://localhost:3000

# Agent Timeouts (seconds)
AGENT_TIMEOUT=30
ORCHESTRATOR_TIMEOUT=60
